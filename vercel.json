{
    "rewrites": [
        {
            "source": "/gacha",
            "destination": "/gacha.html"
        },
        {
            "source": "/random",
            "destination": "/api/random.py"
        },
        {
            "source": "/network",
            "destination": "/api/network.py"
        },
        {
            "source": "/favicon.ico",
            "destination": "/style/favicon.ico"
        },
        {
            "source": "/style/(.*)",
            "destination": "/style/(.*)"
        },
        {
            "source": "/(.*)",
            "destination": "/api/redirect.py"
        }
    ]
}
